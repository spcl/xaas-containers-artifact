{
  "gpu_build": {
    "value": true,
    "build_flag": "-DGGML_CUDA"
  },
  "gpu_backends": {
    "CUDA": {
      "used_as_default": false,
      "build_flag": "-DGGML_CUDA",
      "minimum_version": null
    },
    "Metal": {
      "used_as_default": true,
      "build_flag": "-DGGML_METAL",
      "minimum_version": null
    },
    "Vulkan": {
      "used_as_default": false,
      "build_flag": "-DGGML_VULKAN",
      "minimum_version": null
    },
    "OpenCL": {
      "used_as_default": false,
      "build_flag": "-DGGML_OPENCL",
      "minimum_version": "3.0"
    },
    "SYCL": {
      "used_as_default": false,
      "build_flag": "-DGGML_SYCL",
      "minimum_version": null
    }
  },
  "parallel_programming_libraries": {
    "OpenMP": {
      "used_as_default": true,
      "build_flag": "-DGGML_OPENMP",
      "minimum_version": null
    }
  },
  "linear_algebra_libraries": {
    "BLAS": {
      "used_as_default": true,
      "build_flag": "-DGGML_BLAS",
      "condition": "Apple platforms default to ON, others OFF"
    }
  },
  "FFT_libraries": {
    "FFTW": {
      "built-in": false,
      "used_as_default": false,
      "dependencies": null,
      "build_flag": null,
      "condition": null
    }
  },
  "other_external_libraries": {
    "CURL": {
      "version": "",
      "used_as_default": true,
      "conditions": "When LLAMA_BUILD_COMMON is ON",
      "build_flag": "-DLLAMA_CURL"
    }
  },
  "compiler_flags": [
    "-fsanitize=thread",
    "-fsanitize=address",
    "-fsanitize=undefined",
    "-fno-omit-frame-pointer"
  ],
  "optimization_build_flags": [
    "-DGGML_CUDA_FORCE_MMQ",
    "-DGGML_CUDA_FORCE_CUBLAS",
    "-DGGML_CUDA_F16"
  ],
  "compilers": {
    "gcc": {
      "minimum_version": "11"
    },
    "clang": {
      "minimum_version": "11"
    }
  },
  "architectures": [
    "x86_64",
    "ARM64",
    "PowerPC",
    "RISC-V"
  ],
  "simd_vectorization": {
    "AVX": {
      "build_flag": "-DGGML_AVX",
      "default": true
    },
    "AVX2": {
      "build_flag": "-DGGML_AVX2",
      "default": true
    },
    "AVX512": {
      "build_flag": "-DGGML_AVX512",
      "default": false
    },
    "FMA": {
      "build_flag": "-DGGML_FMA",
      "default": true
    },
    "NEON": {
      "build_flag": "-DGGML_CPU_AARCH64",
      "default": true
    }
  },
  "build_system": {
    "type": "cmake",
    "minimum_version": "3.14"
  },
  "internal_build": {
    "library_name": "ggml",
    "build_flag": "-DLLAMA_USE_SYSTEM_GGML"
  }
}